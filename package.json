{
  "name": "pippip-theme",
  "version": "3.0.0",
  "description": "Drupal 8 base theme for Syd",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "mix",
    "production": "mix --production",
    "start": "mix watch",
    "cypress": "export NODE_ENV=development && cypress open",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "wesbos",
      "wesbos/typescript.js"
    ],
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0,
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 10,
            "browsers": [
              "last 2 chrome versions",
              "last 2 firefox versions",
              "last 2 safari versions",
              "last 2 edge versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript",
      "react-app"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@splidejs/splide": "^4.1.4",
    "@vimeo/player": "^2.20.1",
    "axios": "^0.21.1",
    "baguettebox.js": "^1.11.1",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "cypress-axe": "^0.12.2",
    "date-fns": "^2.20.1",
    "delegate": "^3.2.0",
    "dom-focus-lock": "^1.1.0",
    "easyzoom": "^2.6.0",
    "focus-trap": "^7.5.4",
    "focus-trap-js": "^1.1.0",
    "focus-visible": "^5.2.0",
    "focus-within": "^3.0.2",
    "graphql": "^16.7.1",
    "image-zoom": "^1.1.1",
    "include-media": "^1.4.9",
    "input-plus-minus": "^1.0.8",
    "jest": "^26.6.3",
    "jquery-parallax.js": "^1.5.0",
    "js-cookie": "^2.2.1",
    "js-image-zoom": "^0.7.0",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.21",
    "normalize.scss": "^0.1.0",
    "photoswipe": "^4.1.3",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-accessible-accordion": "^5.0.0",
    "react-day-picker": "^7.4.10",
    "react-dom": "^17.0.2",
    "react-js-pagination": "^3.0.3",
    "react-lazy-load-image-component": "^1.6.0",
    "react-loader-spinner": "^5.4.5",
    "react-loaders": "^3.0.1",
    "react-slick": "^0.29.0",
    "react-sliding-pane": "^7.3.0",
    "react-slidy": "^4.3.3",
    "reflex-grid": "^2.0.5",
    "slick-carousel": "^1.8.1",
    "ts-jest": "^26.5.4",
    "van11y-accessible-accordion-aria": "^3.0.1",
    "vanilla-lazyload": "^17.3.1",
    "zenscroll": "^4.0.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.17",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/focus-within": "^1.0.0",
    "@types/js-cookie": "^2.2.6",
    "@types/photoswipe": "^4.1.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-js-pagination": "^3.0.3",
    "@types/zenscroll": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-react-app": "^10.0.0",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cypress": "^6.9.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^6.0.11",
    "laravel-mix-glob": "^1.1.9",
    "parallax-js": "^3.1.0",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "simple-parallax-js": "^5.6.2",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "./testsConfig.ts"
    ]
  }
}
